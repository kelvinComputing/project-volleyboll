---
import Home from '../components/home/Home.astro';
import Layout from '../layouts/Layout.astro';

const userSession = Astro.cookies.get('serial_club');

if (userSession) {
	if (userSession.value === 'sinSerial') {
		return Astro.redirect('/atleta');
	}

	return Astro.redirect('/club');
}
---

<Layout>
	<Home />
</Layout>
